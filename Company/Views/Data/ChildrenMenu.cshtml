@model System.Collections.Generic.List<Company.Models.MenuView>

@foreach (var m in Model)
{
    if (m.isDropdown)
    {
        <li class="dropdown-submenu">
            <a href="javascript:void(0);">@m.Name</a>
            <ul class="dropdown-menu no-bottom-space">
                @Html.Action("ChildrenMenu", "Data", new { ParentId = @m.Id })
            </ul>
        </li>
    }
    else
    {
        <li><a id="@m.Id" style="text-transform:none !important;" href="@m.Target">@m.Name</a></li>
    }
}
